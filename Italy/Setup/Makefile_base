 
map:
	deryaSE --define GRIDRES=${GRIDRES} CoV.lse
	./runderyaSE 1

part:
	deryaSE --partitionmap --filein ${COUNTRY}_${GRIDRES}km.asc --patches ${PROCS} --connect --seed 3
	mv wmap-final.ppm ${COUNTRY}_${GRIDRES}km-${PROCS}.ppm
	\rm wmap-*

clean:
	\rm -f deryaSE.log
	\rm -f DeryaSE-summary*
	\rm -f DeryaSE-map*
	\rm -f Italy_*km*
	\rm -f runderyaSE

